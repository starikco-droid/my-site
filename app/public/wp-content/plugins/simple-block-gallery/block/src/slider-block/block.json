{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "simple-block-gallery/slider-block",
  "title": "Slider Block",
  "parent": [ "simple-block-gallery/sbg-parent-block" ],
  "icon": "video-alt3",
  "description": "Generate the slider gallery.",
  "version": "1.2.9",
  "textdomain": "simple-block-gallery",
  "attributes": {
    "list_images": {
		"type": "string",
		"source": "html"
	},
	"image": {
		"type": "array"
	},
	"images_ids": {
		"type": "array"
	},
	"animation": {
		"type": "number",
		"default": 5
	},
    "preview": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "customClassName": false,
    "className": false,
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}